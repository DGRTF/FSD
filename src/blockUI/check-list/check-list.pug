mixin check-list()
    .check-list
        .check-list__div-head(onclick="showCheckboxes()")
            span.check-list__span-text="Expandeble checkbox list"
            span.check-list__span-icons="keyboard_arrow_down"
        .check-list__div-list
            include .\..\checkbox\checkbox.pug
            +checkbox({input: 'check-list', label: ' St-rt1'},{mod:''})
            +checkbox({input: 'check-list1', label: ' St-rt3'},{mod:''})
            +checkbox({input: 'check-list2', label: ' St-rt2'},{mod:''})
            +checkbox({input: 'check-list3', label: ' St-rt4'},{mod:''})